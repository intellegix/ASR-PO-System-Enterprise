          "Runtime": "NODEJS_16",
          "BuildCommand": "cd web && npm ci && npm run build",
          "StartCommand": "cd web && npm start",
          "RuntimeEnvironmentVariables": {
            "NODE_ENV": "production",
            "PORT": "3000",
            "AWS_REGION": "us-east-2",
            "S3_BUCKET_NAME": "asr-po-system-exports-prod",
