            "ENABLE_RATE_LIMITING": "true",
            "ENABLE_QUICKBOOKS_SYNC": "true",
            "ENABLE_EMAIL_NOTIFICATIONS": "true",
            "ENABLE_AUDIT_TRAIL": "true",
            "ENABLE_REDIS_CACHE": "true",
            "LOG_LEVEL": "info",
            "DATABASE_URL": "postgresql://postgres:ASRPostgres2024!SecureDB@asr-po-system-prod.cp4yuo0wsvxk.us-east-2.rds.amazonaws.com:5432/postgres",
            "NEXTAUTH_SECRET": "44c865780a4f283c5f399b7aec7445b22c822863565cfc7447adda9fe024a906"
          }
        }
      }
    },
    "AutoDeploymentsEnabled": false
